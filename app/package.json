		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"format:clean": "prettier --write \"src/**/*.{js,ts,svelte}\" --plugin prettier-plugin-remove-comments",
		"format:check": "prettier --check \"src/**/*.{js,ts,svelte}\" --plugin prettier-plugin-remove-comments",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"ck": "svelte-check",
		"check:ts": "tsc --noEmit",
		"check:all": "yarn check && yarn check:ts",
		"lint": "prettier --check . && eslint .",
		"lint:fix": "eslint . --fix",
		"lint:comments": "eslint . --quiet --rule 'no-inline-comments:error' --rule 'line-comment-position:error' --fix",
		"audit:env": "node src/scripts/audit-env.ts"
	},
